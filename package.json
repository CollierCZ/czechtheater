{
	"name": "czech-theater",
	"private": true,
	"description": "A site for Czech Theater, a multicultural amateur community theater in Brno presenting Czech plays in English.",
	"version": "1.0.0",
	"author": "Aaron Collier",
	"scripts": {
		"dev": "vite dev",
		"dev:open": "vite dev --open",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"devDependencies": {
		"@kontent-ai/delivery-sdk": "^14.8.0",
		"@kontent-ai/model-generator": "^6.5.1",
		"@kontent-ai/rich-text-resolver": "^1.0.1",
		"@mdi/js": "^7.4.47",
		"@portabletext/svelte": "^2.1.10",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.2",
		"@testing-library/jest-dom": "^6.4.2",
		"@testing-library/svelte": "^4.2.2",
		"@types/eslint": "^8.56.0",
		"@types/eslint__js": "^8.42.3",
		"@types/eslint-config-prettier": "^6.11.3",
		"@types/node": "^20.12.6",
		"@unpic/svelte": "^0.0.51",
		"@vitest/coverage-v8": "^1.4.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"jsdom": "^24.0.0",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^5.0.0-next.100",
		"svelte-check": "^3.6.0",
		"svelte-media-queries": "^1.6.2",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.6.2",
		"typescript": "^5.4.4",
		"typescript-eslint": "^7.6.0",
		"vite": "^5.0.3",
		"vitest": "^1.4.0",
		"xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
