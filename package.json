{
  "name": "czech-theater",
  "private": true,
  "description": "A site for Czech Theater, a multicultural amateur community theater in Brno presenting Czech plays in English.",
  "version": "1.0.0",
  "author": "Aaron Collier",
  "scripts": {
    "dev": "vite dev",
    "dev:open": "vite dev --open",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@kontent-ai/delivery-sdk": "^14.11.0",
    "@kontent-ai/model-generator": "^7.4.0",
    "@kontent-ai/rich-text-resolver": "^1.2.1",
    "@mdi/js": "^7.4.47",
    "@portabletext/svelte": "^2.1.11",
    "@sveltejs/adapter-static": "^3.0.4",
    "@sveltejs/kit": "^2.5.25",
    "@sveltejs/vite-plugin-svelte": "^4.0.0-next.6",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/svelte": "^5.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.5.1",
    "@unpic/svelte": "^0.0.53",
    "@vitest/coverage-v8": "^2.0.5",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.43.0",
    "globals": "^15.9.0",
    "jsdom": "^25.0.0",
    "postcss": "^8.4.41",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.6.6",
    "svelte": "^5.0.0-next.242",
    "svelte-check": "^3.8.6",
    "svelte-media-queries": "^1.6.2",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.7.0 ",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^2.0.5",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
