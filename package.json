{
  "name": "czech-theater",
  "private": true,
  "description": "A site for Czech Theater, a multicultural amateur community theater in Brno presenting Czech plays in English.",
  "version": "1.0.0",
  "author": "Aaron Collier",
  "scripts": {
    "dev": "vite dev",
    "dev:open": "vite dev --open",
    "build": "vite build",
    "prepare": "svelte-kit sync",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@kontent-ai/delivery-sdk": "^16.2.0",
    "@kontent-ai/model-generator": "^8.1.2",
    "@kontent-ai/rich-text-resolver": "^2.0.2",
    "@mdi/js": "^7.4.47",
    "@portabletext/svelte": "npm:@eirikk/portabletext-2-svelte-5@0.1.0-alpha.2",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/vite": "^4.1.5",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/svelte": "^5.2.7",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.15.7",
    "@unpic/svelte": "^1.0.0",
    "@vitest/coverage-v8": "^3.1.2",
    "eslint": "^9.26.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-svelte": "^3.5.1",
    "globals": "^16.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.7",
    "tailwindcss": "^4.1.5",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.4",
    "vitest": "^3.1.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "type": "module",
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.40.1"
  }
}
