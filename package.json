{
  "name": "czech-theater",
  "private": true,
  "description": "A site for Czech Theater, a multicultural amateur community theater in Brno presenting Czech plays in English.",
  "version": "1.0.0",
  "author": "Aaron Collier",
  "scripts": {
    "dev": "vite dev",
    "dev:open": "vite dev --open",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@kontent-ai/delivery-sdk": "^14.10.0",
    "@kontent-ai/model-generator": "^7.3.0",
    "@kontent-ai/rich-text-resolver": "^1.1.3",
    "@mdi/js": "^7.4.47",
    "@portabletext/svelte": "^2.1.11",
    "@sveltejs/adapter-static": "^3.0.2",
    "@sveltejs/kit": "^2.5.18",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/svelte": "^5.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.10",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^20.14.10",
    "@unpic/svelte": "^0.0.53",
    "@vitest/coverage-v8": "^2.0.2",
    "autoprefixer": "^10.4.16",
    "eslint": "^9.6.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.42.0",
    "globals": "^15.8.0",
    "jsdom": "^24.1.0",
    "postcss": "^8.4.39",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.6.5",
    "svelte": "^5.0.0-next.182",
    "svelte-check": "^3.8.4",
    "svelte-media-queries": "^1.6.2",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1",
    "vite": "^5.3.3",
    "vitest": "^2.0.2",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz"
