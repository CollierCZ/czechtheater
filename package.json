{
  "name": "czech-theater",
  "private": true,
  "description": "A site for Czech Theater, a multicultural amateur community theater in Brno presenting Czech plays in English.",
  "version": "1.0.0",
  "author": "Aaron Collier",
  "scripts": {
    "dev": "vite dev",
    "dev:open": "vite dev --open",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@kontent-ai/delivery-sdk": "^15.2.0",
    "@kontent-ai/model-generator": "^7.4.0",
    "@kontent-ai/rich-text-resolver": "^1.2.1",
    "@mdi/js": "^7.4.47",
    "@portabletext/svelte": "^2.1.11",
    "@sveltejs/adapter-static": "^3.0.5",
    "@sveltejs/kit": "^2.7.2",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@testing-library/jest-dom": "^6.6.2",
    "@testing-library/svelte": "^5.2.4",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^9.6.1",
    "@types/eslint__js": "^8.42.3",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.7.8",
    "@unpic/svelte": "^0.0.53",
    "@vitest/coverage-v8": "^2.1.3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.11.0",
    "jsdom": "^25.0.1",
    "postcss": "^8.4.47",
    "postcss-load-config": "^6.0.1",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "svelte": "^5.0.5",
    "svelte-check": "^4.0.5",
    "svelte-media-queries": "^1.6.2",
    "tailwindcss": "^3.4.14",
    "tslib": "^2.8.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0",
    "vite": "^5.4.9",
    "vitest": "^2.1.3",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
